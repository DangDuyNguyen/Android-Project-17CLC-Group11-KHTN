<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#FF5722"></solid>
            <corners android:radius="50dp"></corners>
            <stroke android:color="#FF9800" android:width="4dp"></stroke>
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#878584"></solid>
            <corners android:radius="50dp"></corners>
            <stroke android:color="#504E4B" android:width="4dp"></stroke>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#061670"></solid>
            <corners android:radius="50dp">
            </corners>
            <stroke android:color="#3F51B5" android:width="4dp"></stroke>
        </shape>
    </item>
</selector>


